{
  "name": "m-components",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "gen": "node ./script/genNewComp/index.js",
    "prepare": "husky install",
    "preview": "vite preview",
